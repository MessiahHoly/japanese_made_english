<p id="notice"><%= notice %></p>

<h1>Listing Expressions</h1>
<%= link_to 'New Expression', new_expression_path, class: 'btn btn-default' %>
<table class='table'>
  <thead>
    <tr>
      <th>Expression</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expressions.each do |expression| %>
      <tr>
        <td><%= link_to expression.expression, expression %></td>                
      </tr>
    <% end %>
  </tbody>
</table>